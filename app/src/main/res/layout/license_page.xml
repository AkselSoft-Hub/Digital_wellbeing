<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/license_app_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/license_app"
            android:textSize="30sp"/>

        <TextView
            android:id="@+id/license_app_license"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/license_app_license" />

        <TextView
            android:id="@+id/license_charts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/license_app_generic_start"
            android:textSize="30sp"/>

        <TextView
            android:id="@+id/license_charts_license"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/license_app_mpandroidCharts" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>